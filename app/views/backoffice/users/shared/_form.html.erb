
            <div id="layoutSidenav_content">
                <main>
                    <header class="page-header page-header-compact page-header-light border-bottom bg-white mb-4">
                        <div class="container-xl px-4">
                            <div class="page-header-content">
                                <div class="row align-items-center justify-content-between pt-3">
                                    <div class="col-auto mb-3">
                                        <h1 class="page-header-title">
                                            <div class="page-header-icon"><i data-feather="book-open"></i></div>
                                            Cadastro do Usuário
                                        </h1>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </header>
                    <!-- Main page content-->
                    <div class="container-xl px-4 mt-4">
                        <!-- Account page navigation-->
                        
    
                                        <%= simple_form_for ([:backoffice, @user]) do |f| %>
                            <div class="col-xl-8">
                                <!-- Account details card-->
                                <div class="card mb-4">
                                    <div class="card-header">Castrando um novo usuário</div>
                                    <div class="card-body">
                                          <%= f.input :name, label: 'Nome' %>
                                            <div class="row gx-3 mb-3 p-2 mb-1">
                                              <div class="row gx-3 p-2 mb-1">
                                                <div class="col-md-6">
                                                  <%= f.input :email, label: 'Email' %>
                                                </div>
                                                <div class="col-md-6">
                                                  <%= f.input :adjutancy, label: 'Profissão' %>
                                                </div>
                                            <div class="row gx-3 p-2 mb-1">
                                                <div class="col-md-6">
                                                    <%= f.input :password, label: 'senha' %>
                                                </div>
                                                 <div class="col-md-6">
                                                    <%= f.input :password_confirmation, label: 'confirmar senha' %>
                                                </div>
                                                 <div class="col-md-6">
                                                    <%= f.input :date, label: 'Data do nascimento:' %>
                                                </div>
                                               
                                            </div>
                                            </div>
                                            <br/>
                                            
                                           
                                            <div class="mb-3">
                                              <label class="small mb-1" for="inputEmailAddress">Loja:</label>
                                              <div class="fw-bold shadow-none p-3 mb-5 bg-body-tertiary rounded"> </div>
                                            </div>
                                        <div class="d-grid gap-2 col-2 mx-auto">
                                        <%= f.button :submit, class: 'btn btn-primary', value: "salva"%>
                                        </div>

                                        <% end %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </main>
                <footer class="footer-admin mt-auto footer-light">
                    <div class="container-xl px-4">
                        <div class="row">
                            <div class="col-md-6 small">Copyright &copy; Your Website 2021</div>
                            <div class="col-md-6 text-md-end small">
                                <a href="#!">Privacy Policy</a>
                                &middot;
                                <a href="#!">Terms &amp; Conditions</a>
                            </div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>

    </body>
</html>